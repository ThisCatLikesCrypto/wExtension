{
    "manifest_version": 3,
    "name": "wExtension",
    "version": "0.0.1",
    "description": "Utilities by Wilbur Williams",
    "permissions": ["tabs", "storage"],
    "background": {
      "service_worker": "background.js"
    },
    "chrome_url_overrides": {
      "newtab": "redirect.html"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
          "16": "icon.png",
          "48": "icon.png",
          "128": "icon.png"
        }
    },
    "icons": {
      "48": "icon.png"
    }
  }
  